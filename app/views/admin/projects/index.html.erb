
    <div class="row custyle">
		<h1 class="well">Manage Projects</h1>
    <table class="table table-striped custab">
    <thead>
        <tr>
            <th>Name</th>
            <th>Developer</th>
			<th>Location</th>
            <th class="text-center">Action</th>
        </tr>
    </thead>
		<% @projects.each do |project| %>
            <tr>
                <td><%= project.name %></td>
                <td><%= project.developer.name %></td>
				<td><%= "#{project.address.locality}, #{project.address.city}" %></td>
                <td class="text-center"><a class='btn btn-info btn-xs' href="<%= edit_admin_projects_path(project) %>"><span class="glyphicon glyphicon-edit"></span> Edit</a> <a href="<%= edit_admin_projects_path(project) %>" class="btn btn-danger btn-xs"><span class="glyphicon glyphicon-remove"></span> Del</a></td>
            </tr>
		<% end %>
        
    </table>
    </div>
	<div class="row">
		<div class="pull-left"><a class='btn btn-info btn-xs' href="<%= new_admin_project_path %>">Add</a></div>
	<div class="pull-right">
		<%= will_paginate @projects %>
	</div>
	</div>
</div>